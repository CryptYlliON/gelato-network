{
  "name": "gelato-contracts",
  "version": "0.0.1",
  "description": "If This Then That automation on Ethereum",
  "repository": "https://github.com/gelatodigital/gelato.git",
  "author": "The guys from gelato-digital",
  "private": false,
  "scripts": {
    "blocknumber-ropsten": "npx buidler blockNumber:ropsten",
    "build": "babel ./scripts --out-dir dist",
    "clean": "rimraf dist",
    "compile": "npx buidler compile",
    "deploy-gelatocore-ropsten": "export BUIDLER_NETWORK=ropsten && export CONTRACT_NAME=GelatoCore && babel-node scripts/deploy/deployWithoutArgs.js",
    "deploy-actionmultimintfortimetrigger-ropsten": "export BUIDLER_NETWORK=ropsten && export CONTRACT_NAME=ActionMultiMintForTimeTrigger && babel-node scripts/deploy/deployWithoutArgs.js",
    "eth-price": "npx buidler eth:price",
    "rebuild": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@nomiclabs/buidler": "^1.0.1",
    "@nomiclabs/buidler-ethers": "^1.0.1",
    "@openzeppelin/contracts-ethereum-package": "^2.4.0",
    "@openzeppelin/upgrades": "^2.6.0",
    "dotenv": "^8.2.0",
    "ethers": "^4.0.23",
    "solc": "^0.5.14"
  },
  "dependencies": {
    "@openzeppelin/cli": "^2.6.0",
    "rimraf": "^3.0.0"
  }
}
