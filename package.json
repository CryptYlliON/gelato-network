{
  "name": "gelato-contracts",
  "version": "0.0.1",
  "description": "If This Then That Automation on Ethereum",
  "repository": "https://github.com/gelatodigital/gelato.git",
  "author": "The guys from gelato-digital",
  "private": false,
  "scripts": {
    "build": "babel ./scripts --out-dir dist",
    "solhint": "solhint contracts/**/*.sol",
    "clean": "rimraf dist",
    "compile": "npx buidler clean && npx buidler compile",
    "rebuild": "yarn clean && yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@nomiclabs/buidler": "^1.0.1",
    "@nomiclabs/buidler-ethers": "^1.0.1",
    "@openzeppelin/contracts": "^2.4.0",
    "dotenv": "^8.2.0",
    "ethers": "^4.0.23",
    "solc": "^0.5.14",
    "solhint": "^2.3.0"
  },
  "dependencies": {
    "rimraf": "^3.0.0"
  }
}
